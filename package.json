{
	"name": "skill-swap",
	"private": true,
	"type": "module",
	"scripts": {
		"prepare": "husky",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lint": "eslint src --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
		"stylelint": "stylelint src/**/*.{css,scss} --aei",
		"stylelint:fix": "stylelint src/**/*.{css,scss} --aei --fix",
		"format": "prettier . --write --ignore-unknown --ignore-path .prettierignore",
		"check": "npm run lint:fix && npm run stylelint:fix && npm run format"
	},
	"lint-staged": {
		"src/**/*.{css,scss}": "stylelint --aei --fix",
		"src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
		"*": "prettier . --write --ignore-unknown --ignore-path .prettierignore"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@esbuild/darwin-arm64": "^0.25.5",
		"@hookform/resolvers": "^5.1.1",
		"@reduxjs/toolkit": "^2.8.2",
		"clsx": "^2.1.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-hook-form": "^7.58.1",
		"react-intersection-observer": "^9.16.0",
		"react-redux": "^9.2.0",
		"react-router-dom": "^7.6.2",
		"yup": "^1.6.1"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.0.0",
		"@storybook/addon-a11y": "9.0.8",
		"@storybook/addon-docs": "9.0.8",
		"@storybook/addon-onboarding": "9.0.8",
		"@storybook/addon-vitest": "9.0.8",
		"@storybook/react-vite": "9.0.8",
		"@types/node": "^22.15.29",
		"@types/react": "^19.1.6",
		"@types/react-dom": "^19.1.6",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react-swc": "^3.10.1",
		"@vitest/browser": "^3.2.2",
		"@vitest/coverage-v8": "^3.2.2",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.57.1",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "10.1.5",
		"eslint-import-resolver-typescript": "4.4.2",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-prettier": "5.4.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "4.6.2",
		"eslint-plugin-storybook": "9.0.8",
		"husky": "9.1.7",
		"lint-staged": "^16.1.2",
		"playwright": "^1.52.0",
		"prettier": "3.5.3",
		"sass": "^1.89.1",
		"storybook": "9.0.8",
		"stylelint": "^16.20.0",
		"stylelint-config-recommended-scss": "^15.0.1",
		"stylelint-config-standard": "^38.0.0",
		"stylelint-order": "^7.0.0",
		"stylelint-scss": "^6.12.0",
		"typescript": "5.3.3",
		"vite": "^6.3.5",
		"vite-react-svg": "^0.0.10",
		"vitest": "^3.2.2"
	}
}
